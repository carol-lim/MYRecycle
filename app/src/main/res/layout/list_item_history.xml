<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/listItemHistoryLayout"
    android:layout_width="match_parent"
    android:layout_height="70dp"
    android:padding="10dp"
    >

    <TextView
        android:id="@+id/txtWeightCategory"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="txtWeightCategory"
        android:textSize="20dp"
        app:layout_constraintBottom_toTopOf="@+id/txtCompanyCollector"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txtCompanyCollector"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="txtCompanyCollector"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtWeightCategory" />

    <TextView
        android:id="@+id/txtEarnPrice"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="1dp"
        android:text="txtEarnPrice"
        android:textSize="20dp"
        app:layout_constraintBottom_toTopOf="@+id/txtCollectDateTime"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txtCollectDateTime"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="txtCollectDateTime"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtEarnPrice" />


</androidx.constraintlayout.widget.ConstraintLayout>