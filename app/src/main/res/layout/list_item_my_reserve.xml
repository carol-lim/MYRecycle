<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/listItemMyReserveLayout"
    android:layout_width="match_parent"
    android:layout_height="70dp"
    android:padding="10dp"
    >

    <TextView
        android:id="@+id/txtWasteCategoryName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="2dp"
        android:text="txtWasteCategoryName"
        android:textSize="20dp"
        app:layout_constraintBottom_toTopOf="@+id/txtReserveDateTime"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txtReserveDateTime"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="txtReserveDateTime"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtWasteCategoryName" />

    <TextView
        android:id="@+id/txtReserveStatus"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="txtReserveStatus"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>